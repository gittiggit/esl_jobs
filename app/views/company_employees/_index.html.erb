    <% @company_employees.each do |company_employee| %>
    <div class="col-md-2 col-sm-2 col-lg-2 col-xs-2">
    <img class="thumb-size" src="<%= User.find(company_employee.employee_id).avatar.blank? ? asset_path('profile-placeholder.jpg') : User.find(company_employee.employee_id).avatar.thumb.url %>"/>
    <p class="member-name"><%= User.find(company_employee.employee_id).username %></p>
     
    <% if @right_user %>
    <%= link_to 'Delete',  company_employee,  method: :delete, data: { confirm: 'Are you sure?' }, :class=>"pull-left" %>
    <% end %>
    </div>
    <% end %>
