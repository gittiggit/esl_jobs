		<div class="main-area fix">
			<div class="container">
				<div class="row">
					<div class="col-md-10 col-sm-10 main-area-body fix">
						<div class="bannar fix">
							<img src="/assets/img/bannar.jpg" alt="" />
						</div>
						<section class="job_area fix">
							<h2><%= @container_title %> </h2>
							<div class="row">
								<div class="col-md-9 col-sm-9 all-job-area">
                   <% @job_posts.each do |job_post| %>
									<div class="col-md-4 col-sm-6 single_job_area">
                    <% if job_post.jobslink.blank? %>
                     <% @abc= job_post_path(job_post) %>
                    <% else %>
                    <% @abc= job_post.jobslink %>
                    <% end %>
                    
                    <%= link_to @abc , :target=> "_blank" do %>
										<div class="single_job fix">
											<div class="published_date fix">
												<p><%= job_post.created_at.strftime("%d %b. %Y") %></p>
											</div>
											<section class="job-descrip fix">
												<p><%= raw(strip_tags(job_post.title)[0,100]) %></p>
											</section>
											<div class="sallery fix">
												<p>$540 per month</p>
											</div>
											<div class="place_apply fix">
												<p class="place"><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span><%= job_post.country.countryname %></p>
												<p class="apply"><%=link_to "Read more", job_post %></p>
											</div>
										</div>
									</div>
                  <% end %>
                  <% end %>
									<div class="col-md-12 col-sm-12">
										<nav class="job-paginition">
											<ul class="pagination">
												<li>
	<%= will_paginate @job_posts,:inner_window => 2 %> 
												</li>
											</ul>
										</nav>
									</div>
								</div>