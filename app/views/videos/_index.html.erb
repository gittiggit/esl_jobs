    <% @videos.each do |video| %>
    <div class="col-md-6 text-center">                        
                          <div class="video-box">
                            <div class="embed-responsive embed-responsive-16by9">
                                <% if video.video_url.blank? %>
                              <a href="" data-toggle="modal" data-target="#Video1"><img src="/assets/video-thumbnail.jpg" class="img-responsive"></a>
                                <% else %>
                                <iframe width="420" height="315" src="https://www.youtube.com/embed/<%= video.video_url %>?showinfo=0" frameborder="0" allowfullscreen></iframe>
                                <% end %>
                              <div class="clerarfix"></div>
                            </div>
                            <p><%= video.vid_description %></p>
                             <a href="/videos/<%= video.id %>" data-remote="true" data-method="delete" data-confirm="Are you sure?" class="close-button-x"><span>X</span></a>
          </div>
      </div>
    <% end %>

<div id="incomplete_videos"></div>